{
  "name": "kosera-web-app",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed-photos": "tsx src/db/seed-photos.ts",
    "db:clear-photos": "tsx src/db/seed-photos.ts --clear",
    "db:drop": "drizzle-kit drop",
    "docker:db:generate": "docker exec kosera-app npm run db:generate",
    "docker:db:migrate": "docker exec kosera-app npm run db:migrate",
    "docker:db:push": "docker exec kosera-app npm run db:push",
    "docker:db:studio": "docker exec kosera-app npm run db:studio",
    "docker:db:seed": "docker exec kosera-app npm run db:seed",
    "docker:db:seed-photos": "docker exec kosera-app npm run db:seed-photos",
    "docker:db:clear-photos": "docker exec kosera-app npm run db:clear-photos",
    "docker:db:drop": "docker exec kosera-app npm run db:drop",
    "test-db": "tsx test-db.ts",
    "test-drizzle": "tsx test-db-drizzle.ts",
    "test:auth-manual": "tsx test-auth.ts",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:performance": "vitest run tests/performance/",
    "test:auth-integration": "tsx test-auth.ts",
    "test:auth-api": "vitest run tests/auth/",
    "docker:test": "docker exec kosera-app npm run test",
    "docker:test:run": "docker exec kosera-app npm run test:run",
    "docker:test:unit": "docker exec kosera-app npm run test:unit",
    "docker:test:integration": "docker exec kosera-app npm run test:integration",
    "docker:test:performance": "docker exec kosera-app npm run test:performance",
    "docker:test:auth-api": "docker exec kosera-app npm run test:auth-api",
    "docker:test:search": "docker exec kosera-app npm run test:search",
    "docker:test:all": "docker exec kosera-app npm run test:all",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detach": "docker-compose up --build -d",
    "docker:dev:down": "docker-compose down",
    "docker:dev:clean": "docker-compose down -v --remove-orphans",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:detach": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:build": "docker build -t kosera-api .",
    "docker:build:prod": "docker build --target production -t kosera-api:prod .",
    "test:search": "tsx tests/search-test-runner.ts",
    "test:search-unit": "vitest run tests/unit/search-api.test.ts",
    "test:search-integration": "vitest run tests/integration/search-api.integration.test.ts",
    "test:all": "tsx tests/runner.ts",
    "docker:logs": "docker logs kosera-app",
    "docker:logs:follow": "docker logs -f kosera-app",
    "docker:shell": "docker exec -it kosera-app /bin/sh",
    "docker:restart": "docker restart kosera-app",
    "docker:status": "docker ps --filter name=kosera"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "next": "15.3.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "sweetalert2": "^11.22.2",
    "swiper": "^11.2.6",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/dotenv": "^6.1.1",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@types/swiper": "^5.4.3",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "jsdom": "^26.1.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.6",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
