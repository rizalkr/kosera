{
  "name": "kosera-web-app",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "hooks:setup": "git config core.hooksPath .husky && echo 'Husky hooks path set'",
  "typecheck": "tsc --noEmit",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:seed": "dotenv -e .env.local -- tsx src/db/seed.ts",
    "db:seed-photos": "dotenv -e .env.local -- tsx src/db/seed-photos.ts",
    "db:clear-photos": "dotenv -e .env.local -- tsx src/db/seed-photos.ts --clear",
    "db:drop": "dotenv -e .env.local -- drizzle-kit drop",

    
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit migrate",
    "db:push:test": "dotenv -e .env.test -- drizzle-kit push",
    "db:seed:test": "dotenv -e .env.test -- tsx src/db/seed.ts",

    
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "test:unit": "NODE_ENV=test vitest run tests/unit/",
    "test:integration": "NODE_ENV=test vitest run tests/integration/",
    "test:all": "tsx tests/runner.ts",
  "ci:test": "npm run typecheck && npm run lint && npm run test:unit && npm run test:integration",

    
    "test:search": "tsx tests/search-test-runner.ts",
    "test:search-unit": "NODE_ENV=test vitest run tests/unit/search-api.test.ts",
    "test:search-integration": "NODE_ENV=test vitest run tests/integration/search-api.integration.test.ts",

    
    "docker:test:setup": "docker compose up -d db_test && sleep 2",
    "docker:db:migrate": "docker exec kosera-app npm run db:migrate",
    "docker:db:push": "docker exec kosera-app npm run db:push",
    "docker:db:push:test": "docker exec kosera-app npm run db:push:test",
    "docker:db:seed": "docker exec kosera-app npm run db:seed",
    "docker:db:migrate:test": "docker exec kosera-app npx dotenv -e .env.test -- drizzle-kit migrate",
    "docker:test:unit": "docker exec -e NODE_ENV=test kosera-app npm run test:unit",
    "docker:test:integration": "docker exec kosera-app npm run test:integration",
    "docker:test:integration:persist": "docker exec -e NODE_ENV=test -e INTEGRATION_PERSIST=1 kosera-app npm run test:integration",
    "docker:test:all": "docker exec kosera-app npm run test:all",

    
    "docker:dev": "docker-compose up --build",
    "docker:dev:detach": "docker-compose up --build -d",
    "docker:dev:down": "docker-compose down",
    "docker:dev:clean": "docker-compose down -v --remove-orphans",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:detach": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs": "docker logs kosera-app",
    "docker:logs:follow": "docker logs -f kosera-app",
    "docker:shell": "docker exec -it kosera-app /bin/sh",
    "docker:restart": "docker restart kosera-app",
    "docker:status": "docker ps --filter name=kosera"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "multer": "^2.0.2",
    "next": "15.3.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "sweetalert2": "^11.22.2",
    "swiper": "^11.2.6",
    "swr": "^2.3.4",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/dotenv": "^6.1.1",
    "@types/leaflet": "^1.9.17",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@types/swiper": "^5.4.3",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "jsdom": "^26.1.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.6",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "husky": "^9.0.0"
  }
}
